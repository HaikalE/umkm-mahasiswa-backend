{
  "name": "umkm-mahasiswa-backend",
  "version": "1.1.0",
  "description": "ðŸš€ Backend API untuk Platform UMKM & Mahasiswa - Node.js + PostgreSQL + Express.js",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "db:create": "node src/database/createdb.js",
    "db:migrate": "node src/database/migrate.js",
    "db:migrate:projects": "node src/database/migrate-project-columns.js",
    "db:migrate:payments": "node src/database/migrate-payment-table.js",
    "db:seed": "node src/database/seed.js",
    "db:fix": "npm run db:migrate:projects",
    "db:fix:payments": "npm run db:migrate:payments",
    "db:reset:payments": "node src/database/reset-payments-table.js",
    "db:emergency:payments": "node src/database/emergency-cleanup-payments.js",
    "db:nuclear": "npm run db:emergency:payments",
    "setup": "./scripts/setup.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh production",
    "test:integration": "./scripts/test.sh",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t umkm-mahasiswa-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f api",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "prepare": "chmod +x scripts/*.sh",
    "deadlines:check": "node -e \"require('./src/utils/deadlineService').triggerDeadlineCheck()\""
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "umkm",
    "mahasiswa",
    "api",
    "backend",
    "indonesia",
    "platform",
    "collaboration",
    "business",
    "students",
    "freelance",
    "projects",
    "payment-system",
    "ai-matching",
    "checkpoint-tracking",
    "pricing-intelligence"
  ],
  "author": {
    "name": "Muhammad Haikal Rahman",
    "email": "dev@umkm-mahasiswa.id",
    "url": "https://github.com/HaikalE"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HaikalE/umkm-mahasiswa-backend.git"
  },
  "bugs": {
    "url": "https://github.com/HaikalE/umkm-mahasiswa-backend/issues"
  },
  "homepage": "https://github.com/HaikalE/umkm-mahasiswa-backend#readme",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "cloudinary": "^1.41.0",
    "firebase-admin": "^11.11.1",
    "socket.io": "^4.7.4",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "moment": "^2.29.4",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-plugin-security": "^1.7.1",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "features": {
    "payment_system": "50-50 split payment with escrow protection",
    "checkpoint_tracking": "Project progress monitoring with milestone approvals",
    "ai_matching": "Smart student-project matching with ML algorithms",
    "pricing_intelligence": "Market-based dynamic price suggestions",
    "deadline_enforcement": "Automated deadline tracking with penalties",
    "real_time_features": "Socket.io powered chat and notifications",
    "role_based_auth": "Multi-level authorization system",
    "file_management": "Cloudinary integration for media uploads",
    "analytics": "Comprehensive business intelligence dashboard"
  }
